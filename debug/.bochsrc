romimage: file=/usr/local/share/bochs/BIOS-bochs-latest, address=0xfffe0000
vgaromimage: file=/usr/local/share/bochs/VGABIOS-lgpl-latest
ata0-master: type=disk, path="c.img", translation=lba, mode=flat
boot: disk
display_library: x, options=gui_debug
cpu: model=atom_n270
com1: enabled=1, mode=file, dev=debug.log
megs: 32
megs: 128
mouse: enabled=0
log: /dev/null
debug: action=ignore
info: action=ignore
error: action=report
panic: action=ask
magic_break: enabled=1

